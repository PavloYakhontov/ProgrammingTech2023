{
  "compilerOptions": {
    "paths": {
      "@screens/*": [
        "src/screens/*"
      ],
      "@locale": [
        "src/locale/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ],
      "@type/*": [
        "src/types/*"
      ],
      "@src/*": [
        "src/*"
      ],
      "@core/*": [
        "src/core/*"
      ],
      "@reacts/*": [
        "src/reacts/*"
      ],
      "@redux/*": [
        "src/redux/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ]
    }
  },
  "extends": "@tsconfig/react-native/tsconfig.json",
}
